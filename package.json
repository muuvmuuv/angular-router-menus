{
	"name": "angular-router-menus-workspace",
	"version": "20.3.3",
	"license": "MIT",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "pnpm --filter=angular-router-menus build",
		"watch": "pnpm --filter=angular-router-menus watch",
		"serve": "pnpm --filter=app serve",
		"build-app": "pnpm --filter=app build",
		"check": "biome check .",
		"format": "biome check --unsafe --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@release-it/bumper": "^7.0.5",
		"@types/node": "^24.10.1",
		"del-cli": "^7.0.0",
		"prettier": "^3.7.3",
		"release-it": "^19.0.6",
		"typescript": "5.9.3"
	},
	"engines": {
		"node": ">=22"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"esbuild"
		],
		"overrides": {
			"angular-router-menus": "link:./projects/angular-router-menus/dist"
		}
	}
}
